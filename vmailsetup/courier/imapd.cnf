
RANDFILE = /usr/lib/courier/imapd.rand

[ req ]
default_bits = 1024
encrypt_key = yes
distinguished_name = req_dn
x509_extensions = cert_type
prompt = no

[ req_dn ]
C=DE
ST=Hessen
L=Kassel
O=Muchtel.de Mail Server
OU=Automatically-generated IMAP SSL key
CN=muchtel.de
emailAddress=tuxBurner@muchtel.de


[ cert_type ]
nsCertType = server

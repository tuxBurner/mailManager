encrypt_key = yes
distinguished_name = req_dn
x509_extensions = cert_type
prompt = no

[ req_dn ]
C=DE
ST=Hessen
L=Kassel
O=Muchtel Mail Server
OU=Automatically-generated POP3 SSL key
CN=muchtel.de
emailAddress=tuxBurner@boggo.de


[ cert_type ]
nsCertType = server
